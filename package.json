{
  "type": "module",
  "name": "starter",
  "version": "2025.04.26",
  "main": "server.ts",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks || true",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "check": "biome check --error-on-warnings .",
    "check:fix": "biome check --write --error-on-warnings .",
    "check-types": "tsc --noEmit",
    "database:sync": "tsx --env-file=.env bin/database-sync",
    "dev": "tsx --env-file=.env server",
    "start": "tsx --env-file=.env server",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/react": "^16.3.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.2",
    "jsdom": "^26.1.0",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "argon2": "^0.43.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.2",
    "zod": "^3.24.3"
  }
}
