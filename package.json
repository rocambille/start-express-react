{
  "type": "module",
  "name": "starter",
  "version": "2025.12.01",
  "main": "server.ts",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks || true",
    "biome:check": "biome check --error-on-warnings .",
    "biome:fix": "biome check --write --error-on-warnings .",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "database:sync": "tsx --env-file=.env bin/database-sync",
    "dev": "tsx --env-file=.env server",
    "make:clone": "tsx  --env-file=.env bin/make-clone",
    "make:purge": "tsx --env-file=.env bin/make-purge",
    "start": "tsx --env-file=.env server",
    "test": "vitest run --coverage",
    "types:check": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.14",
    "fs-extra": "^11.3.2",
    "jsdom": "^27.2.0",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "zod": "^4.1.13"
  }
}
