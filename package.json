{
  "type": "module",
  "name": "starter",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks || true",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "check": "biome check --error-on-warnings .",
    "check:fix": "biome check --write --error-on-warnings .",
    "check-types": "tsc --noEmit",
    "dev": "tsx --env-file=.env server",
    "start": "tsx --env-file=.env server",
    "test": "jest --coverage --verbose"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "dependencies": {
    "argon2": "^0.41.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0",
    "zod": "^3.24.2"
  }
}
