{
  "type": "module",
  "name": "starter",
  "version": "1.0.0",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks || true",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "check": "biome check --error-on-warnings .",
    "check-types": "tsc --noEmit",
    "predev": "[ -d 'node_modules' ] || npm i",
    "dev": "tsx --env-file=.env server",
    "prestart": "[ -d 'node_modules' ] || npm i",
    "start": "NODE_ENV=production tsx --env-file=.env server",
    "test": "jest --coverage --verbose"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.0"
  }
}
